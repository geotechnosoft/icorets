<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="view_alias_name_tree" model="ir.ui.view">
        <field name="name">alias.name.tree</field>
        <field name="model">alias.name</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!--    button for import product-->
    <record model="ir.ui.view" id="view_alias_name_form">
        <field name="name">Alias Name</field>
        <field name="model">alias.name</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="name"/>
                    </group>
                    <group>
                    </group>
                </group>
            </form>
        </field>
    </record>

    <record model="ir.actions.act_window" id="action_alias_name_form">
        <field name="name">Alias Name</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">alias.name</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!--    Menuitem for Import script-->
    <menuitem id="menu_alias_name"
              name="Alias Name"
              action="icorets.action_alias_name_form"
              parent="contacts.res_partner_menu_config"
              sequence="50"/>

</odoo>